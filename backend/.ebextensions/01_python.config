option_settings:
  aws:elasticbeanstalk:application:environment:
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
    # DATABASE_URL should be set in AWS Console Environment Configuration
    # Format for PostgreSQL: postgresql://username:password@host:5432/database_name
    # Format for SQLite: sqlite:///./nyc_taxi.db
    # Leave unset to use default SQLite (will be set via AWS Console)
